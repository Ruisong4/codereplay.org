{
  "private": true,
  "name": "demo",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "deploy": "gh-pages -t -d dist",
    "eslint": "eslint pages/** components/**",
    "preexport": "rimraf dist && NEXT_PUBLIC_BASE_PATH=/ace-tracer yarn build",
    "export": "NEXT_PUBLIC_BASE_PATH=/ace-tracer next export -o dist && touch dist/.nojekyll",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "prettier": "prettier -w --plugin=prettier-plugin-organize-imports . && prettier-package-json --write"
  },
  "dependencies": {
    "glob-promise": "4.2.2",
    "lodash": "4.17.21",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-ace": "9.5.0",
    "react-compound-timer": "1.2.0",
    "react-dom": "17.0.2",
    "runtypes": "6.5.1",
    "@codereplay/types": "2022.3.0",
    "@codereplay/ace-recorder": "2022.3.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.180",
    "@types/react": "17.0.41",
    "@types/styled-components": "5.1.24",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "gh-pages": "3.2.3",
    "prettier": "2.6.0",
    "prettier-package-json": "2.6.3",
    "prettier-plugin-organize-imports": "2.3.4",
    "typescript": "4.6.2"
  }
}
